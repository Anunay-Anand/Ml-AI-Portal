<!--Requiring Layout-->
<% layout('layouts/boilerplate.ejs') %>
<div id="cover-image">
    <!--Navbar-->
    <nav class="navbar navbar-expand-lg fixed-top" id="navbar">
        <div class="container-fluid">
            <a class="navbar-brand ps-md-5 float-sm-start fw-bold text-uppercase fs-4 text-light" href="#"><img
                    src="/utilities/logo.png" id="brand-logo"></a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#mainNavbar"
                aria-controls="mainNavbar" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <!-- ms-auto is the new ml-auto to shift all navlinks to right -->
            <div class="collapse navbar-collapse" id="mainNavbar">
                <div class="navbar-nav ms-auto mainNavLinks text-uppercase">
                    <!--Adding logic to set active link-->
                    <a class="nav-link <%= title === 'Home' ? 'active': ''%>" aria-current="page" href="/home">Home</a>
                    <a class="nav-link <%= title === 'About' ? 'active': ''%>" href="/about">About Us</a>
                    <a class="nav-link <%= title === 'Services' ? 'active navbar-edit': ''%>" href="/services">Our
                        Services</a>
                    <a class="nav-link <%= title === 'Register' ? 'active': ''%>" href="#">Login/Register</a>
                </div>
            </div>
        </div>
    </nav>
</div>
<!--Content below navbar-->
<div data-aos="fade-up" data-aos-duration="1500" class="service-content pt-5 pb-5 mt-5">
    <h1 data-aos="fade-up" class="sixth">Our Services</h1>
    <div class="container pt-3 service-partner">

        <p data-aos="fade-up" class="lead">The world of artificial intelligence is expanding quickly. That's why we're
            fostering
            connections between Learnobot experts and enterprises looking to accelerate their business goals with
            machine learning and AI.</p>
    </div>
</div>
<hr class="hr">
<!-- Service Animated box-->
<div data-aos="fade-up" data-aos-duration="1500" class="service-container-wrapper pt-5 text-center">
    <p class="lead pb-5 text-center">Our partners offer a range of consulting and software solutions and other
        frameworks that
        span
        across the ML workflow.</p>
    <div data-aos="fade-up" data-aos-duration="1500" class="service-steps-content-wrapper shadow-lg mb-5 bg-body">
        <div class="service-steps-container-1">
            <img src="https://images.unsplash.com/photo-1529101091764-c3526daf38fe?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=1191&q=80"
                class="w-100 h-100" alt="">
        </div>

        <div class="service-steps-container-2">
            <div class="button">
                <button type="button"
                    class="rounded-pill active service-button ms-3 me-3 btn btn-secondary btn-md">Prepare and
                    ingest Data</button>
                <button type="button" class="rounded-pill service-button me-3 btn btn-secondary btn-md">Train
                    Model</button>
                <button type="button" class="rounded-pill service-button me-3 btn btn-secondary btn-md">Deploy
                    Model</button>
            </div>
            <div data-aos="fade-up" data-aos-duration="1500" class="button-associated-content pt-3">
                <p class="box-content text-center">Data preparation and ingestion is often the first step in ML. Some of
                    our
                    partners have unique capabilities and automated solutions to help prepare, label, and ingest
                    data.</p>
            </div>

        </div>
    </div>

</div>
<hr class="hr">

<!--Service Cards-->
<div data-aos="fade-up" data-aos-duration="1500" class="service-content pt-5 pb-5">
    <h1 class="sixth">Courses</h1>
    <div data-aos="fade-up" data-aos-duration="1500" class="container pt-3 service-partner">

        <p class="lead text-center">Choose from our courses, rich in content and made by experts.</p>
    </div>
    <div class="service-cards container">
        <div class="row mt-4 text-center">
            <% for(let i=0;i<3;i++){ %>
            <div data-aos="fade-up" data-aos-duration="1500" class="card mx-auto p-0 mt-4 card-animation"
                style="width: 18rem;">
                <div class="inner">
                    <img src="https://images.unsplash.com/photo-1550432163-9cb326104944?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1050&q=80"
                        class="card-img-top" alt="...">
                </div>
                <div class="card-body">
                    <h5 class="card-title text-center">Card title</h5>
                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of
                        the card's content.</p>
                    <a href="#" class="card-link btn btn-secondary rounded-pill">Go somewhere</a>
                </div>
            </div>
            <% } %>
        </div>

    </div>
</div>